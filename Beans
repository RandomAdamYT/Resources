	local posFrame = MainShadow
	local btn = CloseGui
	btn.MouseButton1Click:Connect(function()
		if not Client.InstantClose then
			for i,v in pairs(ByeFrame.Texts:GetChildren()) do	
				v.TextTransparency = 1
			end
			for i,v in pairs(ByeFrame.Images:GetChildren()) do	
				v.ImageTransparency = 1
			end
			CancelClose.BackgroundTransparency = 1
			CancelClose.TextTransparency = 1
			ByeFrame.Visible = true
			ByeFrame.BackgroundTransparency = 1
			game.TweenService:Create(ByeFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
				BackgroundTransparency = 0
			}):Play()
			wait(0.7)
			for i,v in pairs(posFrame.Main.ByeFrame.Images:GetChildren()) do
				game.TweenService:Create(v, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					ImageTransparency = 0
				}):Play()
			end
			wait(0.7)
			for i,v in pairs(ByeFrame.Texts:GetChildren()) do	
				game.TweenService:Create(v, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					TextTransparency = 0
				}):Play()
			end
			wait(0.7)
			game.TweenService:Create(CancelClose, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
				TextTransparency = 0,
				BackgroundTransparency = 0
			}):Play()
			local timeOver = 5

			while timeOver > 0 do
				ClosingText.Text = "Closing.. ("..timeOver.."s)"
				wait(1)
				timeOver = timeOver - 1
			end
			game.TweenService:Create(posFrame, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Size = UDim2.new(0,0,0,0),
				Position = UDim2.new(0, posFrame.AbsolutePosition.X + (posFrame.AbsoluteSize.X / 2), 0, posFrame.AbsolutePosition.Y + (posFrame.AbsoluteSize.Y / 2))
			}):Play()
			wait()
			posFrame.Parent:Destroy()
		else
			game.TweenService:Create(posFrame, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Size = UDim2.new(0,0,0,0),
				Position = UDim2.new(0, posFrame.AbsolutePosition.X + (posFrame.AbsoluteSize.X / 2), 0, posFrame.AbsolutePosition.Y + (posFrame.AbsoluteSize.Y / 2))
			}):Play()
			wait()
			posFrame.Parent:Destroy()
		end
	end)

	CancelClose.MouseButton1Click:Connect(function()
		game.TweenService:Create(posFrame, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
			Size = UDim2.new(0,0,0,0),
			Position = UDim2.new(0, posFrame.AbsolutePosition.X + (posFrame.AbsoluteSize.X / 2), 0, posFrame.AbsolutePosition.Y + (posFrame.AbsoluteSize.Y / 2))
		}):Play()
		wait()
		posFrame.Parent:Destroy()
	end)

	btn.MouseEnter:Connect(function()
		game.TweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
			TextColor3 = Color3.fromRGB(95, 145, 201)
		}):Play()
	end)

	btn.MouseLeave:Connect(function()
		game.TweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
			TextColor3 = Color3.fromRGB(169, 169, 171)
		}):Play()
	end)
	GameName.Size = UDim2.new(0, math.max(12, GameName.TextBounds.X + 10), 0, math.max(24, GameName.TextBounds.Y))
	local gameLabel = GameName
	Logo.MouseEnter:Connect(function()
		game.TweenService:Create(gameLabel, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
			BackgroundTransparency = 0,
			TextTransparency = 0,
		}):Play()
	end)

	Logo.MouseLeave:Connect(function()
		game.TweenService:Create(gameLabel, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
			BackgroundTransparency = 1,
			TextTransparency = 1,
		}):Play()
	end)

	local tabs = {}
	local firstTab = true
	function tabs:CreateTab(tabName)
		local pageFrame = Instance.new("ScrollingFrame")
		local TabButton = Instance.new("TextButton")
		local UICorner = Instance.new("UICorner")
		local TabButtonCover = Instance.new("Frame")
		local tabInfo = Instance.new("Frame")
		local tabNamed = Instance.new("TextLabel")

		local UIListLayout1 = Instance.new("UIListLayout")
		local UIPadding1 = Instance.new("UIPadding")
		local UIListLayout = Instance.new("UIListLayout")

		local UIPadding = Instance.new("UIPadding")

		UIPadding.Parent = tabInfo
		UIPadding.PaddingTop = UDim.new(0, 1)
		local function UpdateSize()
			local cS = UIListLayout1.AbsoluteContentSize

			game.TweenService:Create(pageFrame, TweenInfo.new(0.15, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
				CanvasSize = UDim2.new(0,cS.X,0,cS.Y+10)
			}):Play()
		end
		UpdateSize()
		TabButton.Name = "TabButton"..tabName
		TabButton.Parent = TabPages
		TabButton.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
		TabButton.Position = UDim2.new(0, 0, 0.109540634, 0)
		TabButton.Size = UDim2.new(0.948051929, 0, 0.091872789, 0)
		TabButton.AutoButtonColor = false
		TabButton.Font = Enum.Font.SourceSans
		TabButton.Text = ""
		TabButton.TextColor3 = Color3.fromRGB(0, 0, 0)
		TabButton.TextSize = 14.000
		if firstTab then
			firstTab = false
			TabButton.BackgroundTransparency = 0
			TabButtonCover.BackgroundTransparency = 0
			pageFrame.Visible = true
			UpdateSize()
		else
			TabButton.BackgroundTransparency = 1
			TabButtonCover.BackgroundTransparency = 1
			pageFrame.Visible = false
			UpdateSize()
		end
		UICorner.CornerRadius = UDim.new(0, 99)
		UICorner.Parent = TabButton

		TabButtonCover.Name = "TabButtonCover"
		TabButtonCover.Parent = TabButton
		TabButtonCover.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
		TabButtonCover.BorderSizePixel = 0
		TabButtonCover.Size = UDim2.new(0.0753424689, 0, 1, 0)

		tabInfo.Name = "tabInfo"
		tabInfo.Parent = TabButton
		tabInfo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		tabInfo.BackgroundTransparency = 1.000
		tabInfo.Size = UDim2.new(1, 0, 1, 0)

		tabNamed.Name = "tabNamed"
		tabNamed.Parent = tabInfo
		tabNamed.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		tabNamed.BackgroundTransparency = 1.000
		tabNamed.Position = UDim2.new(0.130136982, 0, 0.211553723, 0)
		tabNamed.Size = UDim2.new(0.876712322, 0, 0.548146069, 0)
		tabNamed.Font = Enum.Font.Gotham
		tabNamed.Text = tabName
		tabNamed.TextColor3 = Color3.fromRGB(91, 95, 103)
		tabNamed.TextScaled = true
		tabNamed.TextSize = 14.000
		tabNamed.TextWrapped = true
		tabNamed.TextXAlignment = Enum.TextXAlignment.Left

		UIListLayout.Parent = tabInfo
		UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right
		UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

		UIListLayout1.Parent = pageFrame
		UIListLayout1.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout1.Padding = UDim.new(0, 5)

		UIPadding1.Parent = pageFrame
		UIPadding1.PaddingTop = UDim.new(0, 5)
		-------------
		pageFrame.Name = "pageFrame"
		pageFrame.Parent = PagesFolder
		pageFrame.Active = true
		pageFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		pageFrame.BackgroundTransparency = 1.000
		pageFrame.BorderSizePixel = 0
		pageFrame.Size = UDim2.new(1, 0, 1, 0)
		pageFrame.ScrollBarThickness = 0
		pageFrame.ClipsDescendants = true

		TabButton.MouseButton1Click:Connect(function()
			for i,v in pairs(PagesFolder:GetChildren()) do
				v.Visible = false
			end
			UpdateSize()
			pageFrame.Visible = true
			for i,v in pairs(TabPages:GetChildren()) do
				if v:IsA("TextButton") then
					game.TweenService:Create(v, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundTransparency = 1
					}):Play()
					game.TweenService:Create(v.TabButtonCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundTransparency = 1
					}):Play()
				end
			end
			game.TweenService:Create(TabButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
				BackgroundTransparency = 0
			}):Play()
			game.TweenService:Create(TabButton.TabButtonCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
				BackgroundTransparency = 0
			}):Play()
			UpdateSize()
			pageFrame.ChildAdded:Connect(UpdateSize)
			pageFrame.ChildRemoved:Connect(UpdateSize)
		end)

		local components = {}

		function components:CreateButton(buttonName, callback)
			local disabled = false
			local functions = {}
			callback = callback or function() print("Button") end
			local buttonComponent = Instance.new("Frame")
			local TextButton = Instance.new("TextButton")
			local UICorner = Instance.new("UICorner")
			local Sample = Instance.new("ImageLabel")

			buttonComponent.Name = "buttonComponent"
			buttonComponent.Parent = pageFrame
			buttonComponent.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			buttonComponent.BackgroundTransparency = 1.000
			buttonComponent.Position = UDim2.new(0, 0, 1.07530859e-07, 0)
			buttonComponent.Size = UDim2.new(0.968999982, 0, 0, 30)

			TextButton.Parent = buttonComponent
			TextButton.ClipsDescendants = true
			TextButton.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
			TextButton.Size = UDim2.new(0.195190653, 200, 1, 0)
			TextButton.AutoButtonColor = false
			TextButton.Font = Enum.Font.SourceSans
			TextButton.Text = "   "..buttonName
			TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
			TextButton.TextSize = 16.000
			TextButton.TextWrapped = true
			TextButton.TextXAlignment = Enum.TextXAlignment.Left

			Sample.Name = "Sample"
			Sample.Parent = TextButton
			Sample.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Sample.BackgroundTransparency = 1.000
			Sample.Image = "http://www.roblox.com/asset/?id=4560909609"
			Sample.ImageColor3 = Color3.fromRGB(200,200,200)
			Sample.ImageTransparency = 0.600


			UICorner.CornerRadius = UDim.new(0, 4)
			UICorner.Parent = TextButton

			local btn = TextButton

			btn.MouseEnter:Connect(function()
				if not disabled then
					game.TweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(92, 143, 204)
					}):Play()
				end
			end)

			btn.MouseLeave:Connect(function()
				if not disabled then
					game.TweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62)
					}):Play()
				end
			end)
			UpdateSize()
			local ms = game.Players.LocalPlayer:GetMouse()
			btn.MouseButton1Click:Connect(function()
				if not disabled then
					callback()
					local c = Sample:Clone()
					c.Parent = btn
					local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
					c.Position = UDim2.new(0, x, 0, y)
					local len, size = 0.35, nil
					if btn.AbsoluteSize.X >= btn.AbsoluteSize.Y then
						size = (btn.AbsoluteSize.X * 1.5)
					else
						size = (btn.AbsoluteSize.Y * 1.5)
					end
					c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
					for i = 1, 10 do
						c.ImageTransparency = c.ImageTransparency + 0.05
						wait(len / 12)
					end
					c:Destroy()       
				end 
			end)

			function functions:Disable() 
				if not disabled then
					game.TweenService:Create(TextButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(37, 40, 47),
						TextColor3 = Color3.fromRGB(94, 101, 116)
					}):Play()
					disabled = true
				end
			end
			function functions:Enable() 
				if disabled then
					game.TweenService:Create(TextButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62),
						TextColor3 = Color3.fromRGB(255,255,255)
					}):Play()
					disabled = false
				end
			end
			return functions
		end

		function components:CreateToggle(toggleName, default, callback)
			default = default or false
			toggleName = toggleName or "nil"
			callback = callback or function() print("Toggle") end
			local togg = default

			local toggleComponent = Instance.new("Frame")
			local mainToggle = Instance.new("TextButton")
			local UICorner = Instance.new("UICorner")
			local UncheckedImg = Instance.new("ImageButton")
			local CheckedImg = Instance.new("ImageButton")
			local Sample = Instance.new("ImageLabel")

			toggleComponent.Name = "toggleComponent"
			toggleComponent.Parent = pageFrame
			toggleComponent.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			toggleComponent.BackgroundTransparency = 1.000
			toggleComponent.Position = UDim2.new(0, 0, 1.07530859e-07, 0)
			toggleComponent.Size = UDim2.new(0.968999982, 0, 0, 30)

			Sample.Name = "Sample"
			Sample.Parent = mainToggle
			Sample.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Sample.BackgroundTransparency = 1
			Sample.Image = "http://www.roblox.com/asset/?id=4560909609"
			Sample.ImageColor3 = Color3.fromRGB(200,200,200)
			Sample.ImageTransparency = 0.8

			local btn = mainToggle
			local ms = game.Players.LocalPlayer:GetMouse()

			mainToggle.Name = "mainToggle"
			mainToggle.Parent = toggleComponent
			mainToggle.BackgroundColor3 = Color3.fromRGB(37, 40, 47)
			mainToggle.Size = UDim2.new(0.195190653, 200, 1, 0)
			mainToggle.AutoButtonColor = false
			mainToggle.Font = Enum.Font.SourceSans
			mainToggle.Text = "   "..toggleName
			mainToggle.ClipsDescendants = true
			mainToggle.TextColor3 = Color3.fromRGB(76, 82, 94)
			mainToggle.TextSize = 16.000
			mainToggle.TextWrapped = true
			mainToggle.TextXAlignment = Enum.TextXAlignment.Left
			mainToggle.MouseButton1Click:Connect(function()
				togg = not togg 
				callback(togg)
				if togg then
					game.TweenService:Create(CheckedImg, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						ImageTransparency = 0
					}):Play()
					game.TweenService:Create(mainToggle, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62),
						TextColor3 = Color3.fromRGB(255,255,255)
					}):Play()
					local c = Sample:Clone()
					c.Parent = btn
					local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
					c.Position = UDim2.new(0, x, 0, y)
					local len, size = 0.35, nil
					if btn.AbsoluteSize.X >= btn.AbsoluteSize.Y then
						size = (btn.AbsoluteSize.X * 1.5)
					else
						size = (btn.AbsoluteSize.Y * 1.5)
					end
					c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
					for i = 1, 10 do
						c.ImageTransparency = c.ImageTransparency + 0.05
						wait(len / 12)
					end
					c:Destroy() 
				else
					game.TweenService:Create(CheckedImg, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						ImageTransparency = 1
					}):Play()
					game.TweenService:Create(mainToggle, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(37, 40, 47),
						TextColor3 = Color3.fromRGB(76, 82, 94)
					}):Play()
					local c = Sample:Clone()
					c.Parent = btn
					local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
					c.Position = UDim2.new(0, x, 0, y)
					local len, size = 0.35, nil
					if btn.AbsoluteSize.X >= btn.AbsoluteSize.Y then
						size = (btn.AbsoluteSize.X * 1.5)
					else
						size = (btn.AbsoluteSize.Y * 1.5)
					end
					c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
					for i = 1, 10 do
						c.ImageTransparency = c.ImageTransparency + 0.05
						wait(len / 12)
					end
					c:Destroy() 
				end
			end)

			UICorner.CornerRadius = UDim.new(0, 4)
			UICorner.Parent = mainToggle

			UncheckedImg.Name = "UncheckedImg"
			UncheckedImg.Parent = mainToggle
			UncheckedImg.AnchorPoint = Vector2.new(0.5, 0.5)
			UncheckedImg.BackgroundTransparency = 1.000
			UncheckedImg.LayoutOrder = 7
			UncheckedImg.Position = UDim2.new(0.939999998, 0, 0.5, 0)
			UncheckedImg.Selectable = false
			UncheckedImg.Size = UDim2.new(0.0970000029, 0, 0.800000012, 0)
			UncheckedImg.ZIndex = 2
			UncheckedImg.AutoButtonColor = false
			UncheckedImg.Image = "rbxassetid://3926305904"
			UncheckedImg.ImageColor3 = Color3.fromRGB(68, 73, 84)
			UncheckedImg.ImageRectOffset = Vector2.new(724, 724)
			UncheckedImg.ImageRectSize = Vector2.new(36, 36)

			CheckedImg.Name = "CheckedImg"
			CheckedImg.Parent = UncheckedImg
			CheckedImg.BackgroundTransparency = 1.000
			CheckedImg.LayoutOrder = 1
			CheckedImg.Selectable = false
			CheckedImg.Size = UDim2.new(1, 0, 1, 0)
			CheckedImg.ZIndex = 2
			CheckedImg.AutoButtonColor = false
			CheckedImg.Image = "rbxassetid://3926311105"
			CheckedImg.ImageColor3 = Color3.fromRGB(92, 143, 204)
			CheckedImg.ImageRectOffset = Vector2.new(4, 836)
			CheckedImg.ImageRectSize = Vector2.new(48, 48)
			CheckedImg.ImageTransparency = 1.000

			if default then
				game.TweenService:Create(CheckedImg, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					ImageTransparency = 0
				}):Play()
				game.TweenService:Create(mainToggle, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(50, 54, 62),
					TextColor3 = Color3.fromRGB(255,255,255)
				}):Play()
				callback(togg)
			end

			local btn = mainToggle
			btn.MouseEnter:Connect(function()
				if togg then
					game.TweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(58, 63, 72)
					}):Play()
				else
					game.TweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62)
					}):Play()
				end
			end)

			btn.MouseLeave:Connect(function()
				if togg then	
					game.TweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62)
					}):Play()
				else
					game.TweenService:Create(btn, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(37, 40, 47)
					}):Play()
				end
			end)   
			UpdateSize() 
		end

		function components:CreateTextbox(title, callback)
			title = title or "nil" 
			callback = callback or function() print("TextBox") end

			local textboxComponent = Instance.new("Frame")
			local UICorner = Instance.new("UICorner")
			local TextBoxInfo = Instance.new("TextLabel")
			local textboxContainer = Instance.new("Frame")
			local TextBox = Instance.new("TextBox")
			local UICorner_2 = Instance.new("UICorner")
			local UIListLayout = Instance.new("UIListLayout")
			local UIPadding = Instance.new("UIPadding")

			textboxComponent.Name = "textboxComponent"
			textboxComponent.Parent = pageFrame
			textboxComponent.BackgroundColor3 = Color3.fromRGB(37, 40, 47)
			textboxComponent.Position = UDim2.new(0, 0, 1.07530859e-07, 0)
			textboxComponent.Size = UDim2.new(0.968999982, 0, 0, 30)

			UICorner.CornerRadius = UDim.new(0, 4)
			UICorner.Parent = textboxComponent

			TextBoxInfo.Name = "TextBoxInfo"
			TextBoxInfo.Parent = textboxComponent
			TextBoxInfo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TextBoxInfo.BackgroundTransparency = 1.000
			TextBoxInfo.Size = UDim2.new(0.213654563, 93, 1, 0)
			TextBoxInfo.Font = Enum.Font.SourceSans
			TextBoxInfo.Text = "   "..title
			TextBoxInfo.TextColor3 = Color3.fromRGB(76, 82, 94)
			TextBoxInfo.TextSize = 16.000
			TextBoxInfo.TextWrapped = true
			TextBoxInfo.TextXAlignment = Enum.TextXAlignment.Left

			textboxContainer.Name = "textboxContainer"
			textboxContainer.Parent = textboxComponent
			textboxContainer.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			textboxContainer.BackgroundTransparency = 1.000
			textboxContainer.Position = UDim2.new(0.451496422, 0, 0, 0)
			textboxContainer.Size = UDim2.new(0.548503816, 0, 1, 0)

			TextBox.Parent = textboxContainer
			TextBox.BackgroundColor3 = Color3.fromRGB(42, 46, 54)
			TextBox.ClipsDescendants = true
			TextBox.Position = UDim2.new(0.559031069, 0, 0.166666672, 0)
			TextBox.Size = UDim2.new(0, 55, 0, 20)
			TextBox.ClearTextOnFocus = false
			TextBox.Font = Enum.Font.SourceSans
			TextBox.Text = ""
			TextBox.TextColor3 = Color3.fromRGB(156, 168, 193)
			TextBox.TextSize = 14.000
			TextBox.TextWrapped = true

			UICorner_2.CornerRadius = UDim.new(0, 4)
			UICorner_2.Parent = TextBox

			UIListLayout.Parent = textboxContainer
			UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right
			UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

			UIPadding.Parent = textboxContainer
			UIPadding.PaddingRight = UDim.new(0, 5)

			local btn = textboxComponent
			TextBox.Focused:Connect(function()
				game.TweenService:Create(TextBoxInfo, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					TextColor3 = Color3.fromRGB(255,255,255)
				}):Play()
				game.TweenService:Create(textboxComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(50, 54, 62)
				}):Play()
				game.TweenService:Create(TextBox, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(60, 64, 74),
					Size = UDim2.new(0, 97,0, 20)
				}):Play()
			end)

			TextBox.FocusLost:Connect(function(EnterPressed, focusThing)
				if EnterPressed then
					callback(TextBox.Text)
					wait()
					TextBox.Text = ""
					game.TweenService:Create(TextBoxInfo, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						TextColor3 = Color3.fromRGB(76, 82, 94)
					}):Play()
					game.TweenService:Create(textboxComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(37, 40, 47)
					}):Play()
					game.TweenService:Create(TextBox, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(42, 46, 54),
						Size = UDim2.new(0, 55,0, 20)
					}):Play()
				else
					game.TweenService:Create(TextBoxInfo, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						TextColor3 = Color3.fromRGB(76, 82, 94)
					}):Play()
					game.TweenService:Create(textboxComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(37, 40, 47)
					}):Play()
					game.TweenService:Create(TextBox, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(42, 46, 54),
						Size = UDim2.new(0, 55,0, 20)
					}):Play()
				end
			end)
			UpdateSize()
		end

		function components:CreateKeybind(title, currentKey, callback)
			local first = currentKey.Name
			callback = callback or function() print("Keybind") end
			local keybindComponent = Instance.new("Frame")
			local KeyBindInfo = Instance.new("TextLabel")
			local TextButton = Instance.new("TextButton")
			local UICorner = Instance.new("UICorner")
			local Frame = Instance.new("Frame")
			local UIListLayout = Instance.new("UIListLayout")
			local UIPadding = Instance.new("UIPadding")
			local keybindOutline = Instance.new("Frame")
			local UICorner_2 = Instance.new("UICorner")
			local UIListLayout_2 = Instance.new("UIListLayout")
			local TextLabel = Instance.new("TextLabel")
			local UICorner_3 = Instance.new("UICorner")
			local UICorner_4 = Instance.new("UICorner")

			keybindComponent.Name = "keybindComponent"
			keybindComponent.Parent = pageFrame
			keybindComponent.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
			keybindComponent.Position = UDim2.new(0, 0, 1.07530859e-07, 0)
			keybindComponent.Size = UDim2.new(0.968999982, 0, 0, 30)

			local textBox = keybindOutline
			local minWidth, minHeight = 36, 18

			TextLabel.TextXAlignment = Enum.TextXAlignment.Center
			TextLabel.TextYAlignment = Enum.TextYAlignment.Center
			 
			local function updateKeySize()
				textBox.Size = UDim2.new(
					0, math.max(minWidth, TextLabel.TextBounds.X + 18),
					0, math.max(minHeight, TextLabel.TextBounds.Y)
				)
			end		

			KeyBindInfo.Name = "KeyBindInfo"
			KeyBindInfo.Parent = keybindComponent
			KeyBindInfo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			KeyBindInfo.BackgroundTransparency = 1.000
			KeyBindInfo.Size = UDim2.new(0.213654563, 93, 1, 0)
			KeyBindInfo.Font = Enum.Font.SourceSans
			KeyBindInfo.Text = "   "..title
			KeyBindInfo.TextColor3 = Color3.fromRGB(255, 255, 255)
			KeyBindInfo.TextSize = 16.000
			KeyBindInfo.TextWrapped = true
			KeyBindInfo.TextXAlignment = Enum.TextXAlignment.Left

			TextButton.Parent = keybindComponent
			TextButton.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
			TextButton.BackgroundTransparency = 1.000
			TextButton.Size = UDim2.new(1, 0, 1, 0)
			TextButton.AutoButtonColor = false
			TextButton.Font = Enum.Font.SourceSans
			TextButton.Text = ""
			TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
			TextButton.TextSize = 14.000
			TextButton.MouseButton1Click:Connect(function()
				TextLabel.Text = ". . ."
				updateKeySize()
				local a, b = game:GetService('UserInputService').InputBegan:wait();
				if a.KeyCode.Name ~= "Unknown" then
					TextLabel.Text = a.KeyCode.Name
					first = a.KeyCode.Name;
					updateKeySize()
				end	
			end)

			game:GetService("UserInputService").InputBegan:connect(function(current, ok) 
				if not ok then 
					if current.KeyCode.Name == first then 
						callback()
					end
				end
			end)

			UICorner.CornerRadius = UDim.new(0, 4)
			UICorner.Parent = TextButton

			Frame.Parent = TextButton
			Frame.AnchorPoint = Vector2.new(0, 0.5)
			Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Frame.BackgroundTransparency = 1.000
			Frame.Position = UDim2.new(0.451000005, 0, 0.5, 0)
			Frame.Size = UDim2.new(0.548503816, 0, 0.699999988, 0)

			UIListLayout.Parent = Frame
			UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right
			UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

			UIPadding.Parent = Frame
			UIPadding.PaddingRight = UDim.new(0, 5)

			keybindOutline.Name = "keybindOutline"
			keybindOutline.Parent = Frame
			keybindOutline.BackgroundColor3 = Color3.fromRGB(38, 40, 45)
			keybindOutline.Position = UDim2.new(0.683972239, 0, 0.0714285746, 0)

			UICorner_2.CornerRadius = UDim.new(0, 4)
			UICorner_2.Parent = keybindOutline

			UIListLayout_2.Parent = keybindOutline
			UIListLayout_2.HorizontalAlignment = Enum.HorizontalAlignment.Center
			UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder

			TextLabel.Parent = keybindOutline
			TextLabel.BackgroundColor3 = Color3.fromRGB(79, 85, 98)
			TextLabel.Size = UDim2.new(1, 0, 0.839999974, 0)
			TextLabel.Font = Enum.Font.GothamBlack
			TextLabel.Text = first
			TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
			TextLabel.TextSize = 10.000
			TextLabel.TextWrapped = false

			UICorner_3.CornerRadius = UDim.new(0, 4)
			UICorner_3.Parent = TextLabel

			UICorner_4.CornerRadius = UDim.new(0, 4)
			UICorner_4.Parent = keybindComponent				
			keybindOutline.Size = UDim2.new(0, math.max(36, TextLabel.TextBounds.X + 18), 0, math.max(18, TextLabel.TextBounds.Y))
			UpdateSize()
		end

		function components:CreateDropdown(title, multipleSelection, list, callback)
			local drops = {}
			list = list or {}
			local selectedTable = {}
			callback = callback or function() print("Dropdown") end
			title = title or "nil"
			multipleSelection = multipleSelection or false
			local isOpened = false

			local dropdownComponent = Instance.new("Frame")
			local UICorner = Instance.new("UICorner")
			local componentInners = Instance.new("Frame")
			local UICorner_2 = Instance.new("UICorner")
			local header = Instance.new("Frame")
			local TextButton = Instance.new("TextButton")
			local UICorner_3 = Instance.new("UICorner")
			local headerCover = Instance.new("Frame")
			local line = Instance.new("Frame")
			local UIListLayout = Instance.new("UIListLayout")
			local arrow = Instance.new("ImageButton")
			local UIListLayout_2 = Instance.new("UIListLayout")
			local UIListLayout_3 = Instance.new("UIListLayout")
			local UIListLayout_4 = Instance.new("UIListLayout")

			dropdownComponent.Name = "dropdownComponent"
			dropdownComponent.Parent = pageFrame
			dropdownComponent.BackgroundColor3 = Color3.fromRGB(24, 28, 33)
			dropdownComponent.BorderColor3 = Color3.fromRGB(77, 85, 95)
			dropdownComponent.BorderSizePixel = 0
			dropdownComponent.ClipsDescendants = true
			dropdownComponent.LayoutOrder = 9999
			dropdownComponent.Position = UDim2.new(0, 0, 0.19379513, 0)
			dropdownComponent.Size = UDim2.new(0.968999982, 0, 0, 32)

			UICorner.CornerRadius = UDim.new(0, 4)
			UICorner.Parent = dropdownComponent

			componentInners.Name = "componentInners"
			componentInners.Parent = dropdownComponent
			componentInners.BackgroundColor3 = Color3.fromRGB(32, 34, 39)
			componentInners.Size = UDim2.new(0.991999984, 0, 0.981999993, 0)

			UICorner_2.CornerRadius = UDim.new(0, 4)
			UICorner_2.Parent = componentInners

			header.Name = "header"
			header.Parent = componentInners
			header.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			header.BackgroundTransparency = 1.000
			header.BorderColor3 = Color3.fromRGB(27, 42, 53)
			header.Position = UDim2.new(0, 0, -0.00169721653, 0)
			header.Size = UDim2.new(1, 0, 0, 30)

			TextButton.Parent = header
			TextButton.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
			TextButton.Position = UDim2.new(-0.00607295381, 0, 0, 0)
			TextButton.Size = UDim2.new(1, 0, 1, 0)
			TextButton.AutoButtonColor = false
			TextButton.Font = Enum.Font.SourceSans
			TextButton.Text = "   "..title
			TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
			TextButton.TextSize = 16.000
			TextButton.TextWrapped = true
			TextButton.TextXAlignment = Enum.TextXAlignment.Left
			TextButton.MouseButton1Click:Connect(function()
				if not isOpened then
					game.TweenService:Create(dropdownComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						Size = UDim2.new(0.968999982,0, 0, UIListLayout_3.AbsoluteContentSize.Y)
					}):Play()
					game.TweenService:Create(line, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundTransparency = 0
					}):Play()
					game.TweenService:Create(headerCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundTransparency = 0
					}):Play()
				else
					game.TweenService:Create(dropdownComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						Size = UDim2.new(0.968999982,0, 0, 32)
					}):Play()
					game.TweenService:Create(line, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundTransparency = 1
					}):Play()
					game.TweenService:Create(headerCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundTransparency = 1
					}):Play()
				end
				isOpened = not isOpened
			end)

			TextButton.MouseEnter:Connect(function()
				if not isOpened then
					game.TweenService:Create(dropdownComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(19, 22, 26)
					}):Play()
					game.TweenService:Create(TextButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(36, 39, 45)
					}):Play()
					game.TweenService:Create(headerCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(36, 39, 45)
					}):Play()
				else
					game.TweenService:Create(TextButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(36, 39, 45)
					}):Play()
					game.TweenService:Create(headerCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(36, 39, 45)
					}):Play()
				end
			end)

			TextButton.MouseLeave:Connect(function()
				if not isOpened then
					game.TweenService:Create(dropdownComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(24, 28, 33)
					}):Play()
					game.TweenService:Create(TextButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62)
					}):Play()
					game.TweenService:Create(headerCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62)
					}):Play()
					wait(0.1)
					UpdateSize()
				else
					game.TweenService:Create(TextButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62)
					}):Play()
					game.TweenService:Create(headerCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(50, 54, 62)
					}):Play()
					wait(0.1)
					UpdateSize()
				end
			end)

			UICorner_3.CornerRadius = UDim.new(0, 4)
			UICorner_3.Parent = TextButton

			headerCover.Name = "headerCover"
			headerCover.Parent = TextButton
			headerCover.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
			headerCover.BackgroundTransparency = 1.000
			headerCover.BorderSizePixel = 0
			headerCover.Position = UDim2.new(0, 0, 0.800000012, 0)
			headerCover.Size = UDim2.new(1.00000024, 0, -0.0333333351, 7)

			line.Name = "line"
			line.Parent = headerCover
			line.BackgroundColor3 = Color3.fromRGB(19, 22, 26)
			line.BackgroundTransparency = 1.000
			line.BorderSizePixel = 0
			line.Position = UDim2.new(0, 0, 0.418571413, 0)
			line.Size = UDim2.new(1, 0, 0, 1)

			UIListLayout.Parent = headerCover
			UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
			UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Bottom

			arrow.Name = "arrow"
			arrow.Parent = TextButton
			arrow.AnchorPoint = Vector2.new(0, 1)
			arrow.BackgroundTransparency = 1.000
			arrow.LayoutOrder = 12
			arrow.Position = UDim2.new(0.89046818, 0, 0.833333373, 0)
			arrow.Size = UDim2.new(0, 20, 0, 20)
			arrow.ZIndex = 2
			arrow.AutoButtonColor = false
			arrow.Image = "rbxassetid://3926305904"
			arrow.ImageRectOffset = Vector2.new(404, 284)
			arrow.ImageRectSize = Vector2.new(36, 36)

			UIListLayout_2.Parent = header
			UIListLayout_2.HorizontalAlignment = Enum.HorizontalAlignment.Center
			UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout_2.VerticalAlignment = Enum.VerticalAlignment.Center

			UIListLayout_3.Parent = componentInners
			UIListLayout_3.HorizontalAlignment = Enum.HorizontalAlignment.Center
			UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder

			UIListLayout_4.Parent = dropdownComponent
			UIListLayout_4.HorizontalAlignment = Enum.HorizontalAlignment.Center
			UIListLayout_4.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout_4.VerticalAlignment = Enum.VerticalAlignment.Center

			local first = componentInners
			local second = dropdownComponent
			UpdateSize()
			for i,v in next, list do
				local option = Instance.new("TextButton")
				UpdateSize()
				option.Name = "option"
				option:SetAttribute("Selected", false)
				option.Parent = componentInners
				option.BackgroundColor3 = Color3.fromRGB(22, 23, 27)
				option.BackgroundTransparency = 1.000
				option.BorderSizePixel = 0
				option.Position = UDim2.new(0, 0, 0.248823628, 0)
				option.Size = UDim2.new(1, 0, 0, 30)
				option.AutoButtonColor = false
				option.Font = Enum.Font.Gotham
				option.Text = "   "..v
				option.TextColor3 = Color3.fromRGB(255, 255, 255)
				option.TextSize = 13.000
				option.TextXAlignment = Enum.TextXAlignment.Left
				option.MouseButton1Click:Connect(function()
					if multipleSelection then
						if option:GetAttribute("Selected") == false then
							game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
								BackgroundTransparency = 0
							}):Play()
							wait()
							table.insert(selectedTable, v)
							for i,v in pairs(selectedTable) do
								callback(v)
							end 
							option:SetAttribute("Selected", true)
						else
							game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
								BackgroundTransparency = 1
							}):Play()
							for a,b in pairs(selectedTable) do
								if b == v then
									table.remove(selectedTable, a) 
								end
							end
							wait()
							for i,v in pairs(selectedTable) do
								callback(v)
							end 
							option:SetAttribute("Selected", false)
						end
					else
						callback(v)
						game.TweenService:Create(dropdownComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							Size = UDim2.new(0.968999982,0, 0, 32)
						}):Play()
						game.TweenService:Create(line, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 1
						}):Play()
						game.TweenService:Create(headerCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 1
						}):Play()
						for i,v in pairs(componentInners:GetChildren()) do
							if v.Name == "option" then
								v:SetAttribute("Selected", false)
								game.TweenService:Create(v, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
									BackgroundTransparency = 1
								}):Play()
							end
						end
						game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 0
						}):Play()
						isOpened = false
						option:SetAttribute("Selected", true)
						wait()
						UpdateSize()
					end	
				end)

				option.MouseEnter:Connect(function()
					if option:GetAttribute("Selected") == false then
						game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 0.5
						}):Play()
					end
				end)

				option.MouseLeave:Connect(function()
					if option:GetAttribute("Selected") == false then
						game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 1
						}):Play()
					end
				end)
			end
			first.Size = UDim2.new(0, 246, 0, second.Size.Y.Offset - 2)
			UpdateSize()
			dropdownComponent.Changed:Connect(function(property)
				if property == "Size" then
					first.Size = UDim2.new(0, 246, 0, second.Size.Y.Offset - 2)
					UpdateSize()
				end
			end)		
			function drops:Refresh(newlist)
				newlist = newlist or {}

				for i,v in pairs(componentInners:GetChildren()) do
					if v.Name == "option" then
						v:Destroy()
					end
				end

				for i,v in next, newlist do
					local option = Instance.new("TextButton")
				UpdateSize()
				option.Name = "option"
				option:SetAttribute("Selected", false)
				option.Parent = componentInners
				option.BackgroundColor3 = Color3.fromRGB(22, 23, 27)
				option.BackgroundTransparency = 1.000
				option.BorderSizePixel = 0
				option.Position = UDim2.new(0, 0, 0.248823628, 0)
				option.Size = UDim2.new(1, 0, 0, 30)
				option.AutoButtonColor = false
				option.Font = Enum.Font.Gotham
				option.Text = "   "..v
				option.TextColor3 = Color3.fromRGB(255, 255, 255)
				option.TextSize = 13.000
				option.TextXAlignment = Enum.TextXAlignment.Left
				option.MouseButton1Click:Connect(function()
					if multipleSelection then
						if option:GetAttribute("Selected") == false then
							game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
								BackgroundTransparency = 0
							}):Play()
							wait()
							table.insert(selectedTable, v)
							for i,v in pairs(selectedTable) do
								callback(v)
							end 
							option:SetAttribute("Selected", true)
						else
							game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
								BackgroundTransparency = 1
							}):Play()
							for a,b in pairs(selectedTable) do
								if b == v then
									table.remove(selectedTable, a) 
								end
							end
							wait()
							for i,v in pairs(selectedTable) do
								callback(v)
							end 
							option:SetAttribute("Selected", false)
						end
					else
						callback(v)
						game.TweenService:Create(dropdownComponent, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							Size = UDim2.new(0.968999982,0, 0, 32)
						}):Play()
						game.TweenService:Create(line, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 1
						}):Play()
						game.TweenService:Create(headerCover, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 1
						}):Play()
						for i,v in pairs(componentInners:GetChildren()) do
							if v.Name == "option" then
								v:SetAttribute("Selected", false)
								game.TweenService:Create(v, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
									BackgroundTransparency = 1
								}):Play()
							end
						end
						game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 0
						}):Play()
						isOpened = false
						option:SetAttribute("Selected", true)
						wait()
						UpdateSize()
					end	
				end)

				option.MouseEnter:Connect(function()
					if option:GetAttribute("Selected") == false then
						game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 0.5
						}):Play()
					end
				end)

				option.MouseLeave:Connect(function()
					if option:GetAttribute("Selected") == false then
						game.TweenService:Create(option, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
							BackgroundTransparency = 1
						}):Play()
					end
				end)
				end
			end
			return drops
		end

		function components:CreateSlider(title, default, minvalue, maxvalue, callback)
			title = title or "Slider"
			default = default or minvalue
			minvalue = minvalue or 0
			maxvalue = maxvalue or 200
			callback = callback or function() end

			local sliderComponent = Instance.new("Frame")
			local UICorner = Instance.new("UICorner")
			local sliderBtn = Instance.new("TextButton")
			local UICorner_2 = Instance.new("UICorner")
			local UIListLayout = Instance.new("UIListLayout")
			local drag = Instance.new("Frame")
			local UICorner_3 = Instance.new("UICorner")
			local UIListLayout_2 = Instance.new("UIListLayout")
			local Frame = Instance.new("Frame")
			local UICorner_4 = Instance.new("UICorner")
			local UIPadding = Instance.new("UIPadding")
			local sliderInfo = Instance.new("TextLabel")
			local UIListLayout_3 = Instance.new("UIListLayout")
			local currentValue = Instance.new("TextLabel")
			local UICorner_5 = Instance.new("UICorner")
			local Value;
			local mouse = game:GetService("Players").LocalPlayer:GetMouse()
			sliderComponent.Name = "sliderComponent"
			sliderComponent.Parent = pageFrame
			sliderComponent.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
			sliderComponent.Position = UDim2.new(0, 0, 0.229029655, 0)
			sliderComponent.Size = UDim2.new(0.968999982, 0, 0, 46)

			UICorner.CornerRadius = UDim.new(0, 4)
			UICorner.Parent = sliderComponent
			local defaultSize = default/maxvalue
			local uis = game:GetService("UserInputService")
			sliderBtn.Name = "sliderBtn"
			sliderBtn.Parent = sliderComponent
			sliderBtn.BackgroundColor3 = Color3.fromRGB(106, 114, 131)
			sliderBtn.Position = UDim2.new(0.0340001173, 0, 0.700000286, 0)
			sliderBtn.Size = UDim2.new(0, 230, 0, 4)
			sliderBtn.AutoButtonColor = false
			sliderBtn.Font = Enum.Font.SourceSans
			sliderBtn.Text = ""
			sliderBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
			sliderBtn.TextSize = 14.000
			sliderBtn.MouseButton1Down:Connect(function()
				Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 230) * drag.AbsoluteSize.X) + tonumber(minvalue)) or 0
                    pcall(function()
                        callback(Value)
                        currentValue.Text = Value
                    end)
                    drag.Size = UDim2.new(0, math.clamp(mouse.X - drag.AbsolutePosition.X, 0, 230), 0, 4)
                    moveconnection = mouse.Move:Connect(function()
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 230) * drag.AbsoluteSize.X) + tonumber(minvalue))
                        pcall(function()
                            callback(Value)
                            currentValue.Text = Value
                        end)
                        drag.Size = UDim2.new(0, math.clamp(mouse.X - drag.AbsolutePosition.X, 0, 230), 0, 4)
                    end)
                    releaseconnection = uis.InputEnded:Connect(function(Mouse)
                        if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                            Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 230) * drag.AbsoluteSize.X) + tonumber(minvalue))
                            pcall(function()
                                callback(Value)
                                currentValue.Text = Value
                            end)
                            drag.Size = UDim2.new(0, math.clamp(mouse.X - drag.AbsolutePosition.X, 0, 230), 0, 4)
                            moveconnection:Disconnect()
                            releaseconnection:Disconnect()
                        end
                    end)
			end)

			UICorner_2.CornerRadius = UDim.new(0, 4)
			UICorner_2.Parent = sliderBtn

			UIListLayout.Parent = sliderBtn
			UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

			drag.Name = "drag"
			drag.Parent = sliderBtn
			drag.BackgroundColor3 = Color3.fromRGB(62, 121, 181)
			drag.Size = UDim2.new(defaultSize, 0, 1, 0)

			UICorner_3.CornerRadius = UDim.new(0, 4)
			UICorner_3.Parent = drag

			UIListLayout_2.Parent = drag
			UIListLayout_2.HorizontalAlignment = Enum.HorizontalAlignment.Right
			UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout_2.VerticalAlignment = Enum.VerticalAlignment.Center

			Frame.Parent = drag
			Frame.BackgroundColor3 = Color3.fromRGB(92, 143, 204)
			Frame.Size = UDim2.new(0, 6, 0, 16)

			UICorner_4.CornerRadius = UDim.new(0, 4)
			UICorner_4.Parent = Frame

			UIPadding.Parent = drag
			UIPadding.PaddingLeft = UDim.new(0, 5)

			sliderInfo.Name = "sliderInfo"
			sliderInfo.Parent = sliderComponent
			sliderInfo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			sliderInfo.BackgroundTransparency = 1.000
			sliderInfo.Size = UDim2.new(0.593065381, 93, 0, 30)
			sliderInfo.Font = Enum.Font.SourceSans
			sliderInfo.Text = "   "..title
			sliderInfo.TextColor3 = Color3.fromRGB(255, 255, 255)
			sliderInfo.TextSize = 16.000
			sliderInfo.TextWrapped = true
			sliderInfo.TextXAlignment = Enum.TextXAlignment.Left

			UIListLayout_3.Parent = sliderInfo
			UIListLayout_3.HorizontalAlignment = Enum.HorizontalAlignment.Right
			UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout_3.VerticalAlignment = Enum.VerticalAlignment.Center

			currentValue.Name = "currentValue"
			currentValue.Parent = sliderInfo
			currentValue.BackgroundColor3 = Color3.fromRGB(106, 114, 131)
			currentValue.BorderSizePixel = 0
			currentValue.Position = UDim2.new(0.861828983, 0, 0.216666669, 0)
			currentValue.Size = UDim2.new(0, 33, 0, 18)
			currentValue.Font = Enum.Font.SourceSansSemibold
			currentValue.Text = default
			currentValue.TextColor3 = Color3.fromRGB(255, 255, 255)
			currentValue.TextSize = 14.000

			UICorner_5.CornerRadius = UDim.new(0, 4)
			UICorner_5.Parent = currentValue
			currentValue.Size = UDim2.new(0, math.max(20, currentValue.TextBounds.X + 18), 0, math.max(18, currentValue.TextBounds.Y))
			UpdateSize()
			currentValue.Changed:Connect(function(prop) 
				if prop == "Text" then
					currentValue.Size = UDim2.new(0, math.max(20, currentValue.TextBounds.X + 18), 0, math.max(18, currentValue.TextBounds.Y))
				end
			end)
		end
		return components
	end
	return tabs
end
return lib
