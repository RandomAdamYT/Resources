		UIListLayout_4.HorizontalAlignment = Enum.HorizontalAlignment.Center
		UIListLayout_4.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout_4.VerticalAlignment = Enum.VerticalAlignment.Center

		check.Name = "check"
		check.Parent = background
		check.AnchorPoint = Vector2.new(0, 1)
		check.BackgroundTransparency = 1.000
		check.LayoutOrder = 3
		check.Position = UDim2.new(0.5, 0, 0.5, 0)
		check.Size = UDim2.new(0, 20, 0, 20)
		check.ZIndex = 2
		check.AutoButtonColor = false
		check.Image = "rbxassetid://3926305904"
		check.ImageColor3 = Color3.fromRGB(40, 42, 47)
		check.ImageRectOffset = Vector2.new(312, 4)
		check.ImageRectSize = Vector2.new(24, 24)
		check.MouseButton1Click:Connect(function()
			if optionToggled then
				game.TweenService:Create(mainRound, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(102, 107, 120)
				}):Play()
				game.TweenService:Create(background, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(40, 42, 47)
				}):Play()
				print("on")
			else
				game.TweenService:Create(mainRound, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(62, 121, 181)
				}):Play()
				game.TweenService:Create(background, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(62, 121, 181)
				}):Play()
				print("off")
			end
			optionToggled = not optionToggled
			callback(optionToggled)
		end)

		TextLabel.Parent = toggle
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.BackgroundTransparency = 1.000
		TextLabel.BorderColor3 = Color3.fromRGB(27, 42, 53)
		TextLabel.Position = UDim2.new(0.092731826, 0, -0.392857134, 0)
		TextLabel.Size = UDim2.new(0, 200, 0, 24)
		TextLabel.Font = Enum.Font.Gotham
		TextLabel.Text = title
		TextLabel.TextColor3 = Color3.fromRGB(102, 107, 120)
		TextLabel.TextSize = 14.000
		TextLabel.TextXAlignment = Enum.TextXAlignment.Left

		if default then
			game.TweenService:Create(mainRound, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
				BackgroundColor3 = Color3.fromRGB(62, 121, 181)
			}):Play()
			game.TweenService:Create(background, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
				BackgroundColor3 = Color3.fromRGB(62, 121, 181)
			}):Play()
			callback(optionToggled)
		end
	end

	function createKeybindSetting(title, currentKey, callback)
		local first = currentKey
		local keybindComponentSetting = Instance.new("Frame")
		local TextButton = Instance.new("TextButton")
		local UICorner = Instance.new("UICorner")
		local Frame = Instance.new("Frame")
		local UIListLayout = Instance.new("UIListLayout")
		local UIPadding = Instance.new("UIPadding")
		local keybindOutline = Instance.new("Frame")
		local UICorner_2 = Instance.new("UICorner")
		local UIListLayout_2 = Instance.new("UIListLayout")
		local TextLabel11 = Instance.new("TextLabel")
		local UICorner_3 = Instance.new("UICorner")
		local UIListLayout_3 = Instance.new("UIListLayout")
		local KeyBindInfo = Instance.new("TextLabel")
		local UIPadding_2 = Instance.new("UIPadding")
		local UICorner_4 = Instance.new("UICorner")
		local firstClicked = false

		keybindComponentSetting.Name = "keybindComponentSetting"
		keybindComponentSetting.Parent = settingsFrame
		keybindComponentSetting.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
		keybindComponentSetting.BackgroundTransparency = 1.000
		keybindComponentSetting.Position = UDim2.new(0, 0, 0.345679015, 0)
		keybindComponentSetting.Size = UDim2.new(1, 0, 0, 30)

		local textBox = keybindOutline
		local minWidth, minHeight = 36, 18

		TextLabel11.TextXAlignment = Enum.TextXAlignment.Center
		TextLabel11.TextYAlignment = Enum.TextYAlignment.Center

		local function updateKeySize()
			textBox.Size = UDim2.new(
				0, math.max(minWidth, TextLabel11.TextBounds.X + 18),
				0, math.max(minHeight, TextLabel11.TextBounds.Y)
			)
			Frame.Size = UDim2.new(0, UIListLayout.AbsoluteContentSize.X + 6, 0.699999988, 0)
		end		

		TextLabel11.Changed:Connect(function(tt)
			if tt == "Text" then
				updateKeySize()
			end
		end)

		TextButton.Parent = keybindComponentSetting
		TextButton.BackgroundColor3 = Color3.fromRGB(50, 54, 62)
		TextButton.BackgroundTransparency = 1.000
		TextButton.Size = UDim2.new(0.99999994, 0, 1, 0)
		TextButton.AutoButtonColor = false
		TextButton.Font = Enum.Font.SourceSans
		TextButton.Text = ""
		TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
		TextButton.TextSize = 14.000
		TextButton.MouseButton1Click:Connect(function()
			TextLabel11.Text = ". . ."
			updateKeySize()
			local a, b = game:GetService('UserInputService').InputBegan:wait();
			firstClicked = true
			if a.KeyCode.Name ~= "Unknown" then
				TextLabel11.Text = a.KeyCode.Name
				first = a.KeyCode.Name
				Client.ToggleKey = a.KeyCode.Name
				writefile("CodexSettings.lua", httpservice:JSONEncode(Client))
				firstClicked = false
				updateKeySize()
			end	
		end)

		game:GetService("UserInputService").InputBegan:connect(function(current, ok) 
			if not ok then 
				if current.KeyCode.Name == first and not firstClicked then 
					callback()
				end
			end
		end)

		UICorner.CornerRadius = UDim.new(0, 4)
		UICorner.Parent = TextButton

		Frame.Parent = TextButton
		Frame.AnchorPoint = Vector2.new(0, 0.5)
		Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Frame.BackgroundTransparency = 1.000
		Frame.LayoutOrder = 1
		Frame.Position = UDim2.new(0.0200501271, 0, 0.5, 0)
		Frame.Size = UDim2.new(0.121232867, 0, 0.699999988, 0)

		UIListLayout.Parent = Frame
		UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center

		UIPadding.Parent = Frame
		UIPadding.PaddingRight = UDim.new(0, 5)

		keybindOutline.Name = "keybindOutline"
		keybindOutline.Parent = Frame
		keybindOutline.BackgroundColor3 = Color3.fromRGB(30, 32, 36)
		keybindOutline.Position = UDim2.new(0.698671222, 0, 0.0714285746, 0)
		keybindOutline.Size = UDim2.new(0, 36, 0, 18)

		UICorner_2.CornerRadius = UDim.new(0, 4)
		UICorner_2.Parent = keybindOutline

		UIListLayout_2.Parent = keybindOutline
		UIListLayout_2.HorizontalAlignment = Enum.HorizontalAlignment.Center
		UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder

		TextLabel11.Parent = keybindOutline
		TextLabel11.BackgroundColor3 = Color3.fromRGB(79, 85, 98)
		TextLabel11.Size = UDim2.new(1, 0, 0, 15)
		TextLabel11.Font = Enum.Font.GothamBlack
		TextLabel11.Text = tostring(first)
		TextLabel11.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel11.TextSize = 10.000
		TextLabel11.TextWrapped = true

		UICorner_3.CornerRadius = UDim.new(0, 4)
		UICorner_3.Parent = TextLabel11

		UIListLayout_3.Parent = TextButton
		UIListLayout_3.FillDirection = Enum.FillDirection.Horizontal
		UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout_3.VerticalAlignment = Enum.VerticalAlignment.Center

		KeyBindInfo.Name = "KeyBindInfo"
		KeyBindInfo.Parent = TextButton
		KeyBindInfo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		KeyBindInfo.BackgroundTransparency = 1.000
		KeyBindInfo.LayoutOrder = 2
		KeyBindInfo.Size = UDim2.new(0.213654563, 93, 1, 0)
		KeyBindInfo.Font = Enum.Font.Gotham
		KeyBindInfo.Text = title
		KeyBindInfo.TextColor3 = Color3.fromRGB(102, 107, 120)
		KeyBindInfo.TextSize = 14.000
		KeyBindInfo.TextWrapped = true
		KeyBindInfo.TextXAlignment = Enum.TextXAlignment.Left

		UIPadding_2.Parent = TextButton
		UIPadding_2.PaddingLeft = UDim.new(0, 8)

		UICorner_4.CornerRadius = UDim.new(0, 4)
		UICorner_4.Parent = keybindComponentSetting
		wait(0)
		keybindOutline.Size = UDim2.new(0, math.max(36, TextLabel11.TextBounds.X + 18), 0, math.max(18, TextLabel11.TextBounds.Y))
		Frame.Size = UDim2.new(0, UIListLayout.AbsoluteContentSize.X + 6, 0.699999988, 0)
	end

	createOptionTitle("UI Customization")
	createToggleOption("Instant Close Window", Settings.InstantClose, function(state)
		Client.InstantClose = state
		print(tostring(state).." - Settings Saved")
		writefile("CodexSettings.lua", httpservice:JSONEncode(Client))
	end)
	createKeybindSetting("Toggle UI", Settings.ToggleKey, function()
		UIMinimize()
	end)

	Main.Name = "Main"
	Main.Parent = MainShadow
	Main.BackgroundColor3 = Color3.fromRGB(30, 33, 37)
	Main.ClipsDescendants = false
	Main.Position = UDim2.new(0.481903255, 0, 0.353039771, 0)
	Main.Selectable = true
	Main.Size = UDim2.new(0.908399999, 0, 0.624000013, 0)
	Main.SizeConstraint = Enum.SizeConstraint.RelativeXX
	UICorner.Parent = Main

	Header.Name = "Header"
	Header.Parent = Main
