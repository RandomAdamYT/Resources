	HeaderCover.Size = UDim2.new(1, 0, 0.419999987, 0)

	CloseGui.Name = "CloseGui"
	CloseGui.Parent = Header
	CloseGui.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	CloseGui.BackgroundTransparency = 1.000
	CloseGui.Position = UDim2.new(0.930000007, 0, 0.100000001, 0)
	CloseGui.Size = UDim2.new(0.0686681718, 0, 0.914285719, 0)
	CloseGui.ZIndex = 4
	CloseGui.Font = Enum.Font.Garamond
