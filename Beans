	function UIMinimize()
        if minimized then
			game.TweenService:Create(Main, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Size = UDim2.new(0.908, 0,0.624, 0)
			}):Play()
			wait(0.1)
			game.TweenService:Create(MainShadow, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				ImageTransparency = 0
			}):Play()
            minimized = false
        else
			if settingsOpened then
				game.TweenService:Create(settingsLabel, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					Position = UDim2.new(-1.5, 0, 0, 0)
				}):Play()
				game.TweenService:Create(settingsFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					Position = UDim2.new(0, 0, 1.5, 0)
				}):Play()
				wait(0.1)
				game.TweenService:Create(mark_2, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundTransparency = 1
				}):Play()
				game.TweenService:Create(mark, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					BackgroundTransparency = 0
				}):Play()
				game.TweenService:Create(SettingsIcon, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					ImageColor3 = Color3.fromRGB(101, 105, 113),
					Position = UDim2.new(0.5,0,0.5,0)
				}):Play()
				game.TweenService:Create(HomeIcon, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					ImageColor3 = Color3.fromRGB(58, 97, 165),
					Position = UDim2.new(0.532,0,0.5,0)
				}):Play()
				game.TweenService:Create(TabPages, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
					Position = UDim2.new(0,0,0,0)
				}):Play()
				for i,v in pairs(PagesFolder:GetChildren()) do
					game.TweenService:Create(v, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						Position = UDim2.new(0,0,0,0)
					}):Play()
				end
			end
			game.TweenService:Create(MainShadow, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				ImageTransparency = 1
			}):Play()
			wait(0.1)
			game.TweenService:Create(Main, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Size = UDim2.new(0,0,0,0),
			}):Play()
            minimized = true
        end
    end

